<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deyvi.frugalcombustive">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/frugal"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/frugal"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.activitys.Apresentacao_"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.Transluced.Inicial">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity_"
            android:label="MainActivity" />
        <activity android:name=".ui.activitys.LoadingBackground_" />
        <activity
            android:name=".ui.activitys.Home_"
            android:label="@string/title_activity_home"
             />


    </application>

</manifest>